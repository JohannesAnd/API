extends ./../MainMenu

block content
    h1= "Trip " + tripID
    table
        tr
            th Latitude
            th Longitude
            th Altitude
            th Speed
            th Coolant Temp
            th Engine Load
            th Fuel Pressure
            th Intake Air Temp
            th RPM
            th Errors
            th Time
        each vertex in vertices
            tr
                td= vertex.latitude
                td= vertex.longitude
                td= vertex.altitude
                td= vertex.speed
                td= vertex.coolant_temp
                td= vertex.engine_load
                td= vertex.fuel_pressure
                td= vertex.intake_air_temp
                td= vertex.rpm
                td= vertex.error_msg
                td= vertex.registration_time